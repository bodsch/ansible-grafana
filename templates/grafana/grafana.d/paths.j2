
[paths]
{% if grafana_paths.data is defined and
      grafana_paths.data | string | length > 0 %}
data                  = {{ grafana_paths.data }}
{% endif %}
{% if grafana_paths.temp_data_lifetime is defined and
      grafana_paths.temp_data_lifetime | string | length > 0 %}
temp_data_lifetime    = {{ grafana_paths.temp_data_lifetime }}
{% endif %}
{% if grafana_paths.logs is defined and
      grafana_paths.logs | string | length > 0 %}
logs                  = {{ grafana_paths.logs }}
{% endif %}
{% if grafana_paths.plugins is defined and
      grafana_paths.plugins | string | length > 0 %}
plugins               = {{ grafana_paths.plugins }}
{% endif %}
{% if grafana_paths.provisioning is defined and
      grafana_paths.provisioning | string | length > 0 %}
provisioning          = {{ grafana_paths.provisioning }}
{% endif %}
